  </tr>

<% @stepthrees.each do |stepthree| %>
  <tr>
    <td><%= stepthree.customer_id %></td>
    <td><%= stepthree.customer_fname %></td>
    <td><%= stepthree.customer_lname %></td>
    <td><%= stepthree.title %></td>
    <td><%= stepthree.complete %></td>
