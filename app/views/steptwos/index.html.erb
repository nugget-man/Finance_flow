<h1>Listing steptwos</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Customer fname</th>
    <th>Customer lname</th>
    <th>Title</th>
    <th>Complete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @steptwos.each do |steptwo| %>
  <tr>
    <td><%= steptwo.customer_id %></td>
    <td><%= steptwo.customer_fname %></td>
    <td><%= steptwo.customer_lname %></td>
    <td><%= steptwo.title %></td>
    <td><%= steptwo.complete %></td>
    <td><%= link_to 'Show', steptwo %></td>
    <td><%= link_to 'Edit', edit_steptwo_path(steptwo) %></td>
