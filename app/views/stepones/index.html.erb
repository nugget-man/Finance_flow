<h1>Listing stepones</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Customer fname</th>
    <th>Customer lname</th>
    <th>Title</th>
    <th>Creditcheck</th>
    <th>Credexp</th>
    <th>Credequ</th>
    <th>Credtra</th>
    <th>Lender</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stepones.each do |stepone| %>
  <tr>
    <td><%= stepone.customer_id %></td>
    <td><%= stepone.customer_fname %></td>
    <td><%= stepone.customer_lname %></td>
    <td><%= stepone.title %></td>
    <td><%= stepone.creditcheck %></td>
    <td><%= stepone.credexp %></td>
    <td><%= stepone.credequ %></td>
    <td><%= stepone.credtra %></td>
    <td><%= stepone.lender %></td>
    <td><%= link_to 'Show', stepone %></td>
    <td><%= link_to 'Edit', edit_stepone_path(stepone) %></td>
    <td><%= link_to 'Destroy', stepone, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

