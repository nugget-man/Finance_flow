<h1>Listing stepfours</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Customer fname</th>
    <th>Customer lname</th>
    <th>Title</th>
    <th>Complete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stepfours.each do |stepfour| %>
  <tr>
    <td><%= stepfour.customer_id %></td>
    <td><%= stepfour.customer_fname %></td>
    <td><%= stepfour.customer_lname %></td>
    <td><%= stepfour.title %></td>
    <td><%= stepfour.complete %></td>
    <td><%= link_to 'Show', stepfour %></td>
    <td><%= link_to 'Edit', edit_stepfour_path(stepfour) %></td>
    <td><%= link_to 'Destroy', stepfour, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

