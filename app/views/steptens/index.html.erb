<h1>Listing steptens</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Customer fname</th>
    <th>Customer lname</th>
    <th>Title</th>
    <th>Complete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @steptens.each do |stepten| %>
  <tr>
    <td><%= stepten.customer_id %></td>
    <td><%= stepten.customer_fname %></td>
    <td><%= stepten.customer_lname %></td>
    <td><%= stepten.title %></td>
    <td><%= stepten.complete %></td>
    <td><%= link_to 'Show', stepten %></td>
    <td><%= link_to 'Edit', edit_stepten_path(stepten) %></td>
    <td><%= link_to 'Destroy', stepten, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

