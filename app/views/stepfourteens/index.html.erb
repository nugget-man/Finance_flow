<h1>Listing stepfourteens</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Customer fname</th>
    <th>Customer lname</th>
    <th>Updated by</th>
    <th>Complete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stepfourteens.each do |stepfourteen| %>
  <tr>
    <td><%= stepfourteen.customer_id %></td>
    <td><%= stepfourteen.customer_fname %></td>
    <td><%= stepfourteen.customer_lname %></td>
    <td><%= stepfourteen.updated_by %></td>
    <td><%= stepfourteen.complete %></td>
    <td><%= link_to 'Show', stepfourteen %></td>
    <td><%= link_to 'Edit', edit_stepfourteen_path(stepfourteen) %></td>
    <td><%= link_to 'Destroy', stepfourteen, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

